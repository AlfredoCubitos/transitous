[Unit]
Description=MOTIS Proxy
After=network-online.target

[Service]
Type=simple
ExecStart=/usr/local/bin/motis-proxy
Restart=always
User=motis
WorkingDirectory=/var/lib/motis-proxy

[Install]
WantedBy=multi-user.target
